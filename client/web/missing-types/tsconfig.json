{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "lib": ["es6", "dom"],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "baseUrl": ".",
    "experimentalDecorators": true,
    "typeRoots": ["."],
    "paths": {
      "@ember/component": ["./types/@ember/component"],
      "@ember/component/*": ["./types/@ember/component/*"]
    },
    "types": ["./types"],
    "noEmit": true,
    "forceConsistentCasingInFileNames": true
  },
  "files": ["./types/*"]
}
