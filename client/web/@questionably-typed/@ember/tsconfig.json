{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "lib": ["es6", "dom"],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "baseUrl": "../",
    "experimentalDecorators": true,
    "typeRoots": ["../"],
    "paths": {
      "@ember/*": ["@ember/*"],
      "@ember/component": ["@ember/component.d.ts"]
    },
    "types": [],
    "noEmit": true,
    "forceConsistentCasingInFileNames": true
  },
  "files": ["index.d.ts"]
}
